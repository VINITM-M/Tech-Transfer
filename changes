import React from 'react';
import './sidebar.css';
import { Link } from 'react-router-dom';
import folderIcon from '../assets/images/master.svg';
import adminIcon from '../assets/images/admin.svg';
import logoutIcon from '../assets/images/po.svg';
import msdsOptionIcon from '../assets/images/msds.png';
import sharinghistory from '../assets/images/sharinghistory.svg';
import AnalyticsIcon from '@mui/icons-material/Analytics';

const Sidebar = () => {
  const userRole = sessionStorage.getItem('userrole');

  return (
    <aside className="p_sideNav ">
      <div className="main">
        
        {userRole === 'System Owner-Admin' && (
          <>
            <div className="btn-group dropend" title="Masters">
              <button>
                <Link to="/masters">
                  <img src={folderIcon} alt="Masters" className="cursor__option" />
                </Link>
              </button>
            </div>

            <div className="btn-group dropend" title="Create User">
              <button>
                <Link to="/users">
                  <img src={adminIcon} alt="Create User" className="cursor__option" />
                </Link>
              </button>
            </div>

            <div className="btn-group dropend" title="Plant Configuration">
              <button>
                <Link to="/Configuration">
                  <img src={folderIcon} alt="Plant Configuration" className="cursor__option" />
                </Link>
              </button>
            </div>

            <div className="btn-group dropend" title="Sharing History">
              <button>
                <Link to="/SharingHistory">
                  <img src={sharinghistory} alt="Sharing History" className="cursor__option" />
                </Link>
              </button>
            </div>

            <div className="btn-group dropend" title="Tracker">
              <button>
                <Link to="/Tracker">
                  <AnalyticsIcon style={{ fontSize: 30, color: '#1e40af' }} />
                </Link>
              </button>
            </div>
          </>
        )}

        
        {(userRole === 'Process Owner' || userRole === 'QA Approver') && (
          <div>
            <div className="btn-group dropend" title="MSDS Summary">
              <button>
                <Link to="/msds">
                  <img src={msdsOptionIcon} alt="MSDS Summary" className="msds" />
                </Link>
              </button>
            </div>

            <div className="btn-group dropend" title="Sharing History">
              <button>
                <Link to="/SharingHistory">
                  <img src={sharinghistory} alt="Sharing History" className="cursor__option" />
                </Link>
              </button>
            </div>
          </div>
        )}
      </div>


      <a
        role="button"
        data-bs-toggle="modal"
        data-bs-target="#logoutmodal"
        className="logout"
        title="Logout"
      >
        <img src={logoutIcon} alt="Logout" />
      </a>
    </aside>
  );
};

export default Sidebar;
